{
  "components": [
    {
      "tag": "bottom-sheet",
      "dependencies": [
        "bottom-sheet-indicator",
        "bottom-sheet-screen"
      ],
      "componentClass": "BottomSheet",
      "componentPath": "components/bottom-sheet/bottom-sheet.js",
      "styles": {
        "$": {
          "stylePaths": [
            "components/bottom-sheet/bottom-sheet.css"
          ]
        }
      },
      "props": [
        {
          "name": "arrow",
          "type": "Boolean",
          "attr": "arrow"
        },
        {
          "name": "initialPosition",
          "type": "String",
          "attr": "initial-position"
        }
      ],
      "states": [
        {
          "name": "dragging"
        },
        {
          "name": "progress"
        }
      ],
      "listeners": [
        {
          "event": "window:resize",
          "method": "resizeHandler",
          "capture": false
        }
      ],
      "methods": [
        {
          "name": "close"
        },
        {
          "name": "open"
        }
      ],
      "connect": [
        {
          "name": "screen",
          "tag": "bottom-sheet-screen"
        }
      ],
      "hostElement": {
        "name": "element"
      },
      "shadow": true
    },
    {
      "tag": "bottom-sheet-indicator",
      "dependencies": [],
      "componentClass": "BottomSheetIndicator",
      "componentPath": "components/bottom-sheet-indicator/bottom-sheet-indicator.js",
      "styles": {
        "$": {
          "stylePaths": [
            "components/bottom-sheet-indicator/bottom-sheet-indicator.css"
          ]
        }
      },
      "shadow": true
    },
    {
      "tag": "bottom-sheet-screen",
      "dependencies": [],
      "componentClass": "BottomSheetScreen",
      "componentPath": "components/bottom-sheet-screen/bottom-sheet-screen.js",
      "styles": {
        "$": {
          "stylePaths": [
            "components/bottom-sheet-screen/bottom-sheet-screen.css"
          ]
        }
      },
      "props": [
        {
          "name": "connectedBottomSheet",
          "type": "Any",
          "attr": "connected-bottom-sheet",
          "watch": [
            "connectedBottomSheetChanged"
          ]
        },
        {
          "name": "progress",
          "type": "Number",
          "attr": "progress",
          "watch": [
            "progressChanged"
          ]
        }
      ],
      "states": [
        {
          "name": "enabled"
        }
      ],
      "listeners": [
        {
          "event": "click",
          "method": "clickHandler",
          "passive": false,
          "capture": false
        }
      ],
      "methods": [
        {
          "name": "disable"
        },
        {
          "name": "enable"
        }
      ],
      "shadow": true
    }
  ],
  "collections": [],
  "compiler": {
    "name": "@stencil/core",
    "version": "0.16.2",
    "typescriptVersion": "3.2.2"
  },
  "bundles": []
}